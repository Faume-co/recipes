{
    "manifests": {
        "faume/shipping": {
            "manifest": {
                "bundles": {
                    "Faume\\ShippingBundle\\FaumeShippingBundle": [
                        "all"
                    ]
                },
                "copy-from-recipe": {
                    "config/": "%CONFIG_DIR%/"
                },
                "composer-scripts": {
                    "cache:clear": "symfony-cmd"
                },
                "env": {
                    "SHIPPY_PRO_ENDPOINT": "https://www.shippypro.com/api/",
                    "SHIPPY_PRO_API_KEY": "!changeMe!"
                },
                "post-install-output": [
                    "  * You are almost ready:",
                    "    -> Change the value of <info>SHIPPY_PRO_API_KEY</info> in the <info>.env</info> file.",
                    "",
                    "  * <fg=blue>Read</> the documentation at <comment>https://github.com/Faume-co/faume-shipping</>"
                ]
            },
            "files": {
                "config/packages/faume_shipping.yaml": {
                    "contents": [
                        "faume_shipping:",
                        "    shippy_pro_endpoint: '%env(SHIPPY_PRO_ENDPOINT)%'",
                        "    shippy_pro_api_key: '%env(SHIPPY_PRO_API_KEY)%'",
                        ""
                    ],
                    "executable": false
                }
            },
            "ref": "2f7c07692e2d6cccefa99a8e19effff879813517"
        }
    }
}
