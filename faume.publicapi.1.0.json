{
    "manifests": {
        "faume/publicapi": {
            "manifest": {
                "bundles": {
                    "Faume\\PublicAPIBundle\\FaumePublicAPIBundle": [
                        "all"
                    ]
                },
                "copy-from-recipe": {
                    "config/": "%CONFIG_DIR%"
                },
                "composer-scripts": {
                    "cache:clear": "symfony-cmd"
                },
                "env": {
                    "FAUME_PUBLIC_API_EMAIL": "!changeMe!",
                    "FAUME_PUBLIC_API_PASSWORD": "!changeMe!",
                    "FAUME_PUBLIC_API_ENV": "!changeMe!"
                },
                "post-install-output": [
                    ""
                ]
            },
            "files": {
                "config/packages/faume_public_api.yaml": {
                    "contents": [
                        "faume_public_api:",
                        "    email: '%env(FAUME_PUBLIC_API_EMAIL)%'",
                        "    password: '%env(FAUME_PUBLIC_API_PASSWORD)%'",
                        "    env: '%env(FAUME_PUBLIC_API_ENV)%'"
                    ],
                    "executable": false
                }
            },
            "ref": "02855b7c07b1be6632bf24935fa8f51d2c3ce7ed"
        }
    }
}
