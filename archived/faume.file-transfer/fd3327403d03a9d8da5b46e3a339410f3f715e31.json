{
    "manifests": {
        "faume/file-transfer": {
            "manifest": {
                "bundles": {
                    "Faume\\FileTransfer\\FaumeFileTransferBundle": [
                        "all"
                    ]
                },
                "copy-from-recipe": {
                    "config/": "%CONFIG_DIR%"
                },
                "composer-scripts": {
                    "cache:clear": "symfony-cmd"
                },
                "env": {
                    "FAUME_FILE_TRANSFER_FILE_EXTENSION": "!changeMe!",
                    "FAUME_FILE_TRANSFER_SEPARATOR": "!changeMe!",
                    "FAUME_FILE_TRANSFER_GENERATE_FILE_PATH": "!changeMe!",
                    "FAUME_FILE_TRANSFER_DISCOUNT_CODE_FILE_PREFIX": "!changeMe!",
                    "FAUME_FILE_TRANSFER_DISCOUNT_CODE_RESULT_FILE_PREFIX": "!changeMe!",
                    "FAUME_FILE_TRANSFER_CUSTOMER_FILE_PREFIX": "!changeMe!",
                    "FAUME_FILE_TRANSFER_CUSTOMER_RESULT_FILE_PREFIX": "!changeMe!",
                    "FAUME_FILE_TRANSFER_PUT_FILE_HOST": "!changeMe!",
                    "FAUME_FILE_TRANSFER_PUT_FILE_USERNAME": "!changeMe!",
                    "FAUME_FILE_TRANSFER_PUT_FILE_PASSWORD": "!changeMe!",
                    "FAUME_FILE_TRANSFER_PUT_FILE_PATH": "!changeMe!",
                    "FAUME_FILE_TRANSFER_GET_FILE_HOST": "!changeMe!",
                    "FAUME_FILE_TRANSFER_GET_FILE_USERNAME": "!changeMe!",
                    "FAUME_FILE_TRANSFER_GET_FILE_PASSWORD": "!changeMe!",
                    "FAUME_FILE_TRANSFER_GET_FILE_PATH": "!changeMe!",
                    "FAUME_FILE_TRANSFER_GET_FILE_DESTINATION_PATH": "!changeMe!"
                },
                "post-install-output": [
                    "  * You are almost ready:",
                    "    -> Set values in your .env file for all FAUME_FILE_TRANSFER variables",
                    "",
                    "  * <fg=blue>Read</> the documentation at <comment>https://github.com/Faume-co/faume-file-transfer</>"
                ]
            },
            "files": {
                "config/packages/faume_file_transfer.yaml": {
                    "contents": [
                        "faume_file_transfer:",
                        "    file_rules:",
                        "        extension:  '%env(FAUME_FILE_TRANSFER_FILE_EXTENSION)%'",
                        "        separator : '%env(FAUME_FILE_TRANSFER_SEPARATOR)%'",
                        "        generate_file_path: '%env(FAUME_FILE_TRANSFER_GENERATE_FILE_PATH)%'",
                        "    discount_code:",
                        "        file_prefix:          '%env(FAUME_FILE_TRANSFER_DISCOUNT_CODE_FILE_PREFIX)%'",
                        "        result_file_prefix:   '%env(FAUME_FILE_TRANSFER_DISCOUNT_CODE_RESULT_FILE_PREFIX)%'",
                        "    customer:",
                        "        file_prefix:          '%env(FAUME_FILE_TRANSFER_CUSTOMER_FILE_PREFIX)%'",
                        "        result_file_prefix:   '%env(FAUME_FILE_TRANSFER_CUSTOMER_RESULT_FILE_PREFIX)%'",
                        "    put_file:",
                        "        host:       '%env(FAUME_FILE_TRANSFER_PUT_FILE_HOST)%'",
                        "        username:   '%env(FAUME_FILE_TRANSFER_PUT_FILE_USERNAME)%'",
                        "        password:   '%env(FAUME_FILE_TRANSFER_PUT_FILE_PASSWORD)%'",
                        "        path:       '%env(FAUME_FILE_TRANSFER_PUT_FILE_PATH)%'",
                        "    get_file:",
                        "        host:       '%env(FAUME_FILE_TRANSFER_GET_FILE_HOST)%'",
                        "        username:   '%env(FAUME_FILE_TRANSFER_GET_FILE_USERNAME)%'",
                        "        password:   '%env(FAUME_FILE_TRANSFER_GET_FILE_PASSWORD)%'",
                        "        path:       '%env(FAUME_FILE_TRANSFER_GET_FILE_PATH)%'",
                        "        destination_path: '%env(FAUME_FILE_TRANSFER_GET_FILE_DESTINATION_PATH)%'"
                    ],
                    "executable": false
                }
            },
            "ref": "fd3327403d03a9d8da5b46e3a339410f3f715e31"
        }
    }
}
