{
    "manifests": {
        "faume/salesforce": {
            "manifest": {
                "bundles": {
                    "Faume\\Salesforce\\FaumeSalesforceBundle": [
                        "all"
                    ]
                },
                "copy-from-recipe": {
                    "config/": "%CONFIG_DIR%/"
                },
                "composer-scripts": {
                    "cache:clear": "symfony-cmd"
                },
                "env": {
                    "FAUME_SALESFORCE_CLIENT_SHORTCODE": "",
                    "FAUME_SALESFORCE_CLIENT_ORGANIZATION_ID": "",
                    "FAUME_SALESFORCE_CLIENT_SITE_ID": "",
                    "FAUME_SALESFORCE_CLIENT_AUTH_DOMAIN": "https://account.demandware.com/dwsso/oauth2/access_token",
                    "FAUME_SALESFORCE_CLIENT_AUTH_ID": "",
                    "FAUME_SALESFORCE_CLIENT_AUTH_SECRET": "",
                    "FAUME_SALESFORCE_CLIENT_AUTH_INSTANCE_ID": "",
                    "FAUME_SALESFORCE_CLIENT_AUTH_REALM_ID": "",
                    "FAUME_SALESFORCE_CAMPAIGN_PREFIX": "faume_seconde_main_%de_%s",
                    "FAUME_SALESFORCE_PROMOTION_PREFIX": "faume_seconde_main_%de",
                    "FAUME_SALESFORCE_COUPON_PREFIX": "faume_seconde_main_%de"
                }
            },
            "files": {
                "config/packages/faume_salesforce.yaml": {
                    "contents": [
                        "faume_salesforce:",
                        "  client:",
                        "    shortcode: \"%env(FAUME_SALESFORCE_CLIENT_SHORTCODE)%\"",
                        "    organization_id: \"%env(FAUME_SALESFORCE_CLIENT_ORGANIZATION_ID)%\"",
                        "    site_id: \"%env(FAUME_SALESFORCE_CLIENT_SITE_ID)%\"",
                        "    auth_domain: \"%env(FAUME_SALESFORCE_CLIENT_AUTH_DOMAIN)%\"",
                        "    auth_client_id: \"%env(FAUME_SALESFORCE_CLIENT_AUTH_ID)%\"",
                        "    auth_client_secret: \"%env(FAUME_SALESFORCE_CLIENT_AUTH_SECRET)%\"",
                        "    auth_instance_id: \"%env(FAUME_SALESFORCE_CLIENT_AUTH_INSTANCE_ID)%\"",
                        "    auth_realm_id: \"%env(FAUME_SALESFORCE_CLIENT_AUTH_REALM_ID)%\"",
                        "  campaign:",
                        "    prefix: \"%env(FAUME_SALESFORCE_CAMPAIGN_PREFIX)%\"",
                        "  promotion:",
                        "    prefix: \"%env(FAUME_SALESFORCE_PROMOTION_PREFIX)%\"",
                        "  coupon:",
                        "    prefix: \"%env(FAUME_SALESFORCE_COUPON_PREFIX)%\""
                    ],
                    "executable": false
                }
            },
            "ref": "84b02f3d970663e862ea035f995363b21046734b"
        }
