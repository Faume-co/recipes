{
    "manifests": {
        "faume/salesforce": {
            "manifest": {
                "bundles": {
                    "Faume\\Salesforce\\FaumeSalesforceBundle": [
                        "all"
                    ]
                },
                "copy-from-recipe": {
                    "config/": "%CONFIG_DIR%/"
                },
                "composer-scripts": {
                    "cache:clear": "symfony-cmd"
                },
                "env": {
                    "FAUME_SALESFORCE_CLIENT_SHORTCODE": "",
