{
    "manifests": {
        "faume/salesforce": {
            "manifest": {
                "bundles": {
                    "Faume\\Fastmag\\FaumeSalesforceBundle": [
                        "all"
                    ]
                },
                "copy-from-recipe": {
                    "config/": "%CONFIG_DIR%/"
                },
                "composer-scripts": {
                    "cache:clear": "symfony-cmd"
                },
                "env": {
                    "FAUME_SALESFORCE_CLIENT_SHORTCODE": "changeMe",
                    "FAUME_SALESFORCE_CLIENT_ORGANIZATION_ID": "changeMe",
                    "FAUME_SALESFORCE_CLIENT_SITE_ID": "changeMe",
                    "FAUME_SALESFORCE_CLIENT_AUTH_DOMAIN": "https://account.demandware.com/dwsso/oauth2/access_token",
                    "FAUME_SALESFORCE_DISCOUNT_CODE_END_AT": "changeMe",
                    "FAUME_SALESFORCE_DISCOUNT_CODE_MULTIPLE_CODE_PER_BASKET": "changeMe",
                    "FAUME_SALESFORCE_DISCOUNT_CODE_PREFIX": "changeMe",
                    "FAUME_SALESFORCE_DISCOUNT_CODE_DESCRIPTION": "changeMe",
                    "FAUME_SALESFORCE_CAMPAIGN_PREFIX": "changeMe",
                    "FAUME_SALESFORCE_CAMPAIGN_DESCRIPTION": "changeMe",
                    "FAUME_SALESFORCE_PROMOTION_PREFIX": "changeMe"
                }
            },
            "files": {
                "config/packages/faume_salesforce.yaml": {
                    "contents": [
                        "faume_salesforce:",
                        "  client:",
                        "    shortcode: '%env(FAUME_SALESFORCE_CLIENT_SHORTCODE)%'",
                        "    organization_id: '%env(FAUME_SALESFORCE_CLIENT_ORGANIZATION_ID)%'",
                        "    site_id: '%env(FAUME_SALESFORCE_CLIENT_SITE_ID)%'",
                        "    auth_domain: '%env(FAUME_SALESFORCE_CLIENT_AUTH_DOMAIN)%'",
                        "  discount_code:",
                        "    discount_code_end_at: '%env(FAUME_SALESFORCE_DISCOUNT_CODE_END_AT)%'",
                        "    multiple_codes_per_basket: '%env(FAUME_SALESFORCE_DISCOUNT_CODE_MULTIPLE_CODE_PER_BASKET)%'",
                        "    prefix: '%env(FAUME_SALESFORCE_DISCOUNT_CODE_PREFIX)%'",
                        "    description: '%env(FAUME_SALESFORCE_DISCOUNT_CODE_DESCRIPTION)%'",
                        "  campaign:",
                        "    prefix: '%env(FAUME_SALESFORCE_CAMPAIGN_PREFIX)%'",
                        "    description: '%env(FAUME_SALESFORCE_CAMPAIGN_DESCRIPTION)%'",
                        "  promotion:",
                        "    prefix: '%env(FAUME_SALESFORCE_PROMOTION_PREFIX)%'"
                    ],
                    "executable": false
                }
            },
            "ref": "0f96a55fd44bf87cb8b8f4e5033e8b0180965efb"
        }
    }
}
