{
    "manifests": {
        "faume/hybris": {
            "manifest": {
                "bundles": {
                    "Faume\\Hybris\\FaumeHybrisBundle": [
                        "all"
                    ]
                },
                "copy-from-recipe": {
                    "config/": "%CONFIG_DIR%/"
                },
                "composer-scripts": {
                    "cache:clear": "symfony-cmd"
                },
                "env": {
                    "HYBRIS_HOST": "changeMe",
                    "HYBRIS_TOKEN": "changeMe"
                },
                "post-install-output": [
                    "  * You are almost ready:",
                    "    -> Set values in your .env file for all FAUME_HYBRIS variables"
                ]
            },
            "files": {
                "config/packages/faume_hybris.yaml": {
                    "contents": [
                        "faume_hybris:",
                        "  host: '%env(HYBRIS_HOST)%'",
                        "  security_token: '%env(HYBRIS_TOKEN)%'"
                    ],
                    "executable": false
                }
            },
            "ref": "084d6c1d3a30fa75ef813fc1fdd3aedc086cc33e"
        }
    }
}
