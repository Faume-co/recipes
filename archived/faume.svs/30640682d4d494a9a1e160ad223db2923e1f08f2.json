{
    "manifests": {
        "faume/svs": {
            "manifest": {
                "bundles": {
                    "Faume\\Svs\\FaumeSvsBundle": [
                        "all"
                    ]
                },
                "copy-from-recipe": {
                    "config/": "%CONFIG_DIR%/"
                },
                "composer-scripts": {
                    "cache:clear": "symfony-cmd"
                },
                "env": {
                    "FAUME_SVS_MERCHANT_ID": "",
                    "FAUME_SVS_VIRTUAL_INVENTORY_ID": "",
                    "FAUME_SVS_STORE_NUMBER": "",
                    "FAUME_SVS_LOGIN": "",
                    "FAUME_SVS_PASSWORD": "",
                    "FAUME_SVS_SECRET": "",
                    "FAUME_SVS_AUTH_ID": "",
                    "FAUME_SVS_ENV": ""
                },
                "post-install-output": [
                    "  * You are almost ready:",
                    "    -> Change the value of these values <info>.env</info> file :",
                    "    - <info>FAUME_SVS_MERCHANT_ID</info>",
                    "    - <info>FAUME_SVS_VIRTUAL_INVENTORY_ID</info>",
                    "    - <info>FAUME_SVS_STORE_NUMBER</info>",
                    "    - <info>FAUME_SVS_LOGIN</info>",
                    "    - <info>FAUME_SVS_PASSWORD</info>",
                    "    - <info>FAUME_SVS_SECRET</info>",
                    "    - <info>FAUME_SVS_AUTH_ID</info>",
                    "    - <info>FAUME_SVS_ENV</info>"
                ]
            },
            "files": {
                "config/packages/faume_svs.yaml": {
                    "contents": [
                        "faume_svs:",
                        "    merchantId: '%env(FAUME_SVS_MERCHANT_ID)%'",
                        "    virtualInventoryId: '%env(FAUME_SVS_VIRTUAL_INVENTORY_ID)%'",
                        "    storeNumber: '%env(FAUME_SVS_STORE_NUMBER)%'",
                        "    login: '%env(FAUME_SVS_LOGIN)%'",
                        "    password: '%env(FAUME_SVS_PASSWORD)%'",
                        "    secret: '%env(FAUME_SVS_SECRET)%'",
                        "    authId: '%env(FAUME_SVS_AUTH_ID)%'",
                        "    env: '%env(FAUME_SVS_ENV)%'"
                    ],
                    "executable": false
                }
            },
            "ref": "30640682d4d494a9a1e160ad223db2923e1f08f2"
        }
    }
}
